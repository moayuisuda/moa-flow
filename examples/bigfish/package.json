{
  "name": "bigfish-playground",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 bigfish build",
    "build": "bigfish build",
    "ci": "bigfish lint && bigfish test --coverage",
    "cov": "bigfish test --coverage",
    "dev": "bigfish dev",
    "devs": "cross-env MOCK=none bigfish dev",
    "help": "bigfish help",
    "postinstall": "bigfish g tmp",
    "lint": "bigfish lint --style --eslint.ext='.ts,.tsx,.jsx,.js'",
    "lint:fix": "bigfish lint --style --prettier --eslint.ext='.ts,.tsx,.jsx,.js' --fix",
    "oneapi": "bigfish api generate service && bigfish api generate mock",
    "oneapi:mock": "bigfish api generate mock",
    "oneapi:service": "bigfish api generate service",
    "pack": "bigfish pack",
    "precommit": "bigfish lint --staged --prettier --style --fix --eslint.ext='.ts,.tsx,.jsx,.js'",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "start": "bigfish dev",
    "test": "bigfish test",
    "version": "bigfish -v"
  },
  "config": {
    "assets": {
      "hash": true
    }
  },
  "dependencies": {
    "@alipay/bigfish": "^3.0.0",
    "@alipay/tech-ui": "^2.0.0",
    "flow": "^0.0.0"
  },
  "devDependencies": {
    "@ali/ci": "^4.21.0",
    "cross-env": "^6.0.3"
  },
  "engines": {
    "install-alinode": "5"
  },
  "ci": {
    "type": "aci",
    "version": "alinode=5"
  },
  "yuyanId": "",
  "description": "技术栈：Bigfish 3.0 + TechUI + Ant Design + 中台最佳实践",
  "main": ".prettierrc.js",
  "directories": {
    "test": "tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
